    <endEvent id="endevent1" name="End"></endEvent>
    <userTask id="sampleTask" name="打样申请" activiti:assignee="${inputUser}"></userTask>
    <userTask id="leadTask" name="领导审核" activiti:assignee="${inputUser}"></userTask>
    <userTask id="makeTask" name="订单生产" activiti:assignee="${inputUser}"></userTask>
    <sequenceFlow id="flow1" sourceRef="startevent1" targetRef="sampleTask"></sequenceFlow>
    <exclusiveGateway id="isPass" name="是否通过"></exclusiveGateway>
    <sequenceFlow id="flow2" sourceRef="sampleTask" targetRef="leadTask"></sequenceFlow>
    <sequenceFlow id="flow3" sourceRef="leadTask" targetRef="isPass"></sequenceFlow>
    <sequenceFlow id="flow4" name="否" sourceRef="isPass" targetRef="sampleTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isPass == 1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow5" name="是" sourceRef="isPass" targetRef="makeTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isPass == 0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow6" sourceRef="makeTask" targetRef="checkTask"></sequenceFlow>
    <userTask id="checkTask" name="质检" activiti:assignee="${inputUser}"></userTask>
    <exclusiveGateway id="isCheck" name="是否通过质检"></exclusiveGateway>
    <userTask id="outTask" name="出货" activiti:assignee="${inputUser}"></userTask>
    <sequenceFlow id="flow7" sourceRef="checkTask" targetRef="isCheck"></sequenceFlow>
    <sequenceFlow id="flow8" name="是" sourceRef="isCheck" targetRef="outTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isCheck == 0}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" name="否" sourceRef="isCheck" targetRef="makeTask">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isCheck == 1}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow10" sourceRef="outTask" targetRef="endevent1"></sequenceFlow>
  </process>
